Algoritmo ej4sem1_2
	Definir n1, n2, div como real;
	Escribir "Ingrese dos numeros";
	Leer n1;
	Leer n2;
	Si n2<>0 Entonces 
		div=n1/n2;
		Escribir "El resultado de la division es " div;
	SiNo
		Escribir "El seguno numero no es diferente de 0, por lo tanto no se puede hacer la division";
	FinSi
FinAlgoritmo


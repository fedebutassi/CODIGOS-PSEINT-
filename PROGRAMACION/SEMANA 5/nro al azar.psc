Algoritmo arr_sem5_1
	Definir vector_numeros, aleat como entero;
	Definir i Como Entero;
	Dimension vector_numeros[10];
	Escribir "Numeros aleatorios elevados al cuadrado";
	Para i=1 hasta 9 hacer
		aleat=1+azar(10)+1;
		Escribir (aleat)^2;
	FinPara
	Escribir "Numeros aleatorios elevados al cubo";
	Para i=1 hasta 9 hacer
		aleat=1+azar(10)+1;
		Escribir (aleat)^3;
	FinPara
	
FinAlgoritmo